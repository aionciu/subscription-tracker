# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Expo Dev URL (for published apps on expo.dev)
# This will be your published app URL from expo.dev
EXPO_PUBLIC_EXPO_DEV_URL=exp://u.expo.dev/abf13a38-972c-4fbb-a0ae-e8d87567233d

# Expo Go Development URL (for local development)
# Replace with your actual IP address when running locally
# You can find this by running: npx expo start --tunnel
EXPO_PUBLIC_EXPO_GO_URL=exp://192.168.1.100:8081

# Google OAuth Configuration (for web)
EXPO_PUBLIC_GOOGLE_CLIENT_ID_WEB=your_google_client_id_for_web
EXPO_PUBLIC_GOOGLE_CLIENT_ID_IOS=your_google_client_id_for_ios
EXPO_PUBLIC_GOOGLE_CLIENT_ID_ANDROID=your_google_client_id_for_android
